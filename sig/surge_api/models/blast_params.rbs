      def to_hash: -> {
        attachments: ::Array[SurgeAPI::AttachmentParams],
        body: String,
        contacts: ::Array[String],
        name: String,
        segments: ::Array[String],
        send_at: Time,
        to: ::Array[String]
      }
    end
  end
end
