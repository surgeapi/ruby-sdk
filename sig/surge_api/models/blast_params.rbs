        body: String,
        contacts: ::Array[String],
        name: String,
        segments: ::Array[String],
        send_at: Time,
        to: ::Array[String]
      }

    class BlastParams < SurgeAPI::Internal::Type::BaseModel
      attr_reader attachments: ::Array[SurgeAPI::AttachmentParams]?

      def attachments=: (
